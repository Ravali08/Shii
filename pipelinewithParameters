pipeline {
    agent any
    parameters {
	string(defaultValue: 'test', description: 'Description of the parameter', name: 'value')
  	booleanParam defaultValue: true, name: 'Bool'
  	choice choices: ['Train', 'Val', 'Prod', 'Dev'], name: 'Env'
 	}	
	stages {
  		 stage('Test_Parameters'){
   			 steps {
      				echo "Name is : ${value}"
     				echo "Choice is : ${Env}"
     				echo "Boolean is : ${Bool}"
  			  }
  		}
	}
}
